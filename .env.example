# Example environment for local development
# Copy to `.env` and fill values

# Runtime
NODE_ENV=development
LOG_LEVEL=info

# Database
# - Development: if DATABASE_URL is omitted, the app falls back to local sqlite at ./database/bot.sqlite
# - Production: DATABASE_URL is required (Postgres)
# Example:
# postgres://user:password@localhost:5432/pb_bots
DATABASE_URL=

# Optional: If your app reads discrete fields
DATABASE_NAME=
DATABASE_USER=
DATABASE_PASSWORD=

# Discord application IDs and bot tokens
#
# Preferred env var names (runtime entrypoints use these):
SAM_BOT_TOKEN=
SAM_CLIENT_ID=
SAM_GUILD_ID=
SAM_REGISTER_ON_BOOT=false

DEAN_BOT_TOKEN=
# Discord "Application ID" (aka Client ID). Some scripts use DEAN_CLIENT_ID; PM2 config uses DEAN_APP_ID.
DEAN_APP_ID=
DEAN_CLIENT_ID=
DEAN_GUILD_ID=
DEAN_REGISTER_ON_BOOT=false

CAS_BOT_TOKEN=
# Discord "Application ID" (aka Client ID). Some scripts use CAS_CLIENT_ID; PM2 config uses CAS_APP_ID.
CAS_APP_ID=
CAS_CLIENT_ID=
CAS_GUILD_ID=
CAS_REGISTER_ON_BOOT=false

# Legacy aliases (still supported in a few registration scripts):
BOT_TOKEN=
CLIENT_ID=
GUILD_ID=

# AO3 credentials (only needed if the parser authenticates; safe to omit for public works)
AO3_USERNAME=your-ao3-username
AO3_PASSWORD=your-ao3-password
AO3_HEADLESS=true

# AO3 rate limiting / timeouts (optional)
AO3_MIN_REQUEST_INTERVAL_MS=20000
AO3_NAV_TIMEOUT=90000
AO3_SERIES_NAV_TIMEOUT=
AO3_LOGIN_RETRY_MAX=3
AO3_LOGIN_RETRY_BASE_DELAY=5000

# Queue cleanup thresholds (optional)
PARSEQUEUE_PENDING_STUCK_MIN=
PARSEQUEUE_PROCESSING_STUCK_MIN=
PARSEQUEUE_SERIES_PROCESSING_STUCK_MIN=

# Command Prefix (optional)
COMMAND_PREFIX=!

